{
  "name": "@syntro-opensource/bootbox",
  "version": "1.0.0",
  "description": "A set of leightweight modifications for Bootstrap components",
  "main": "index.js",
  "scripts": {
    "develop": "npm-run-all --parallel docs-serve watch",
    "docs": "npm-run-all build docs-build",
    "docs-serve": "bundle exec jekyll s -s docs/ -d temp_docs -o",
    "docs-build": "bundle exec jekyll build -s docs/ -d build",
    "watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntro-opensource/bootbox.git"
  },
  "keywords": [
    "bootstrap",
    "scss",
    "sass"
  ],
  "author": "Matthias Leutenegger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syntro-opensource/bootbox/issues"
  },
  "homepage": "https://github.com/syntro-opensource/bootbox#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "dependencies": {
    "sassdoc": "^2.7.2"
  }
}
